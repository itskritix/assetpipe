{
  "name": "assetpipe",
  "version": "0.1.0",
  "private": true,
  "description": "Open-source platform for company logos and brand assets",
  "scripts": {
    "dev:web": "pnpm --filter @assetpipe/web dev",
    "dev:api": "pnpm --filter @assetpipe/api dev",
    "build": "pnpm -r build",
    "build:web": "pnpm --filter @assetpipe/web build",
    "build:api": "pnpm --filter @assetpipe/api build",
    "deploy:web": "pnpm --filter @assetpipe/web deploy",
    "deploy:api": "pnpm --filter @assetpipe/api deploy",
    "db:migrate": "supabase db push",
    "db:reset": "supabase db reset",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm -r typecheck"
  },
  "keywords": [
    "logos",
    "brand-assets",
    "api",
    "open-source"
  ],
  "license": "MIT",
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=20.0.0"
  }
}
